{
  "name": "www.remove-background.ai",
  "version": "1.0.0",
  "description": "A simple wrapper for the https://www.remove-background.ai API",
  "type": "module",
  "scripts": {
    "build": "tsc --p .",
    "before-test": "npm run build && tsc --p ./examples",
    "test-remove-from-url": "npm run before-test && node examples/removeFromUrl.js",
    "test-remove-from-file": "npm run before-test && node examples/removeFromFile.js",
    "test-remove-from-base64": "npm run before-test && node examples/removeFromBase64.js",
    "test-remove-from-file-with-crop-and-scale": "npm run before-test && node examples/removeFromFileWithCropAndScale.js",
    "test-remove-from-file-with-new-background": "npm run before-test && node examples/removeFromFileWithNewBackground.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtrabelsi/rembg.js.git"
  },
  "keywords": [
    "rembg",
    "remove background ai",
    "background removal ai",
    "remove background automatically with ai",
    "remove background with ai",
    "image processing",
    "background image"
  ],
  "author": "Marwen Trabelsi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mtrabelsi/rembg.js/issues"
  },
  "homepage": "https://github.com/mtrabelsi/rembg.js#readme",
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "tmp-promise": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "esm": "^3.2.25",
    "typescript": "~3.2.4"
  }
}
